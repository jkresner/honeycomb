{
  "appDir":                  "../",
  "auth": {
    "appKey":                "auth-ex1",
    "api": {
      "users": {
        "unlinkOAuth":      true,
        "updatePrimaryEmail":true
      }
    },
    "oauth": {
      "github": {
        "short":             "gh",
        "signup":            true,
        "login":             true,
        "unlink":            false,
        "logic":             "oauth",
        "clientID":          "{{required}}",
        "clientSecret":      "{{required}}",
        "emails":            true,
        "userAgent":         "{{required}}"
      },
      "facebook": {
        "short":             "fb",
        "signup":            true,
        "login":             true,
        "logic":             "oauth",
        "clientID":          "{{required}}",
        "clientSecret":      "{{required}}"
      },
      "stackoverflow": {
        "short":             "so",
        "logic":             "oauth",
        "clientID":          "{{required}}",
        "clientSecret":      "{{required}}",
        "key":               "{{required}}"
      },
      "linkedin": {
        "short":             "in",
        "logic":             "oauth",
        "clientID":          "{{required}}",
        "clientSecret":      "{{required}}",
        "profile":           "picture-url,num-connections",
        "scope":             ["r_basicprofile"]
      }
    },
    "password": {
      "signup":              "{{undefine}}",
      "reset":               "{{undefine}}"
    }
  },
  "comm":                    "{{undefine}}",
  "http": {
    "host":                  "localhost",
    "static":                { "favicon": "{{undefine}}" }
  },
  "log":{
    "appKey":                "HON",
    "analytics": {            
      "model": {
        "mongoUrl":           "",
        "collections":        { "event": "Event", "issue": "Issue" }
      }
    }
  },
  "middleware": {
    "session": {
      "authdData":           "_id name",
      "store":               { "collection": "sessions-ex1" }
    }
  },
  "model": {
    "analytics": {
      "mongoUrl":            "{{required}}"
    },
    "da":                    ["mongoose","mongo"]
  }
}
